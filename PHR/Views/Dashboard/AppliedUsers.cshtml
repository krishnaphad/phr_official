
@{
    ViewData["Title"] = "Applied Users";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="row justify-content-lg-start">
    <form id="appliedJobsForm">
        <div class="row">
            <div class="col-sm-6">
                <div class="form-group">
                    <label class="form-label" for="appliedJobSkills">Job Skills</label>
                    <select class="" name="appliedJobSkills[]" id="appliedJobSkills" multiple="multiple">
                    </select>
                    <span id="appliedJobSkillsError" class="text-danger"></span>
                </div>
            </div>

            <div class="col-sm-3">
                <div class="form-group">
                    <label class="form-label" for="appliedJobSearchBtn">Action</label>
                    <button id="appliedJobSearchBtn" class="form-control border-style btn btn-primary" type="button"><i class="fa fa-search"></i>Search</button>
                </div>
            </div>
        </div>
    </form>
</div>

